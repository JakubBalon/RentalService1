@model RentalService.Models.Login

<h1>Logowanie</h1>

@using (Html.BeginForm("Login", "Account", FormMethod.Post))
{
    @Html.ValidationSummary();

    <span>Zaloguj: </span> @Html.TextBoxFor(x => x.UserName )
    
    <span>Hasło: </span> @Html.PasswordFor(x => x.Password )

    <button type="submit">Zaloguj</button>

}
